*
MOVE *CPU-TIME TO #CPU-START
STARTB. SETTIME
FOR #I = 1 TO 100000
MOVE all ' ' TO #TAB(*)
END-FOR
WRITE 5T 'MOVE blank TO #TAB(*): ' / 25T 'ELAPSED TIME ' *TIMD (STARTB.)
COMPUTE #CPU-ELAPSED = *CPU-TIME - #CPU-START


WRITE 25T 'CPU TIME   ' #CPU-ELAPSED /
*
*
MOVE *CPU-TIME TO #CPU-START
STARTC. SETTIME

FOR #I = 1 TO 100000
RESET #TAB(1:200)
END-FOR

WRITE 5T 'RESET #TAB(1:200) : ' / 25T 'ELAPSED TIME ' *TIMD (STARTC.)
COMPUTE #CPU-ELAPSED = *CPU-TIME - #CPU-START

WRITE 25T 'CPU TIME   ' #CPU-ELAPSED /
*
*
MOVE *CPU-TIME TO #CPU-START
STARTD. SETTIME

FOR #I = 1 TO 100000
RESET #TAB(*)
END-FOR

WRITE 5T 'RESET #TAB(*) : '/ 25T 'ELAPSED TIME ' *TIMD (STARTD.)
COMPUTE #CPU-ELAPSED = *CPU-TIME - #CPU-START

WRITE 25T 'CPU TIME   ' #CPU-ELAPSED /
*
*
MOVE *CPU-TIME TO #CPU-START
STARTE. SETTIME

FOR #I = 1 TO 10000
