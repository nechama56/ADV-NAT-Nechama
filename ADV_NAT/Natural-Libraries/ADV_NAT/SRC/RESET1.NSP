DEFINE DATA LOCAL
* 1 EMPLOY-VIEW VIEW OF EMPLOYEES
*   2 NAME
1 #TAB   (A4/1:200)     
1 #INDEX (I4)
1 #NUM (P3)
1 #COUNTER (P5)
1 #CPU-START (P9)
1 #CPU-END (P9)
1 #CPU-ELAPSED (P9)

1 #I (P9)
END-DEFINE
*
*
*
MOVE *CPU-TIME TO #CPU-START
STARTA. SETTIME

FOR #I = 1 TO 100000
MOVE ' ' TO #TAB(1:200)
END-FOR

COMPUTE #CPU-ELAPSED = *CPU-TIME - #CPU-START
WRITE 5T 'MOVE blank TO #TAB(1:200): ' / 25T 'ELAPSED TIME ' *TIMD (STARTA.)
WRITE 25T 'CPU TIME   ' #CPU-ELAPSED / 

